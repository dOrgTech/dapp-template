{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "baseUrl": "src",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "experimentalDecorators": true,
    "strictNullChecks": false
  }
}
